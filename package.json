  },
  "homepage": "https://github.com/paulohfcanuto/todo-app-backend#readme",
  "description": "",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.32.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.2.0",
    "@prisma/client": "^6.1.0",
    "fastify": "^5.2.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "pino-pretty": "^10.3.1",
    "prisma": "^6.1.0",
    "tsx": "^4.7.1",
    "typescript": "^5.7.2"
  }
}
